<div class="message-wrapper">
      <div class="message-upper">
        <div class="user-name">
          <%= message.user.name %>
        </div>
        <div class="created-at">
          <%= message.created_at %>
        </div>
      </div>
      <div class="message-lower">
        <div class="message">
          <%= message.content %>
        </div>
      </div>
  </div>